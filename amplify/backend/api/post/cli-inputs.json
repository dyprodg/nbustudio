{
  "version": 1,
  "paths": {
    "/create": {
      "name": "/create",
      "lambdaFunction": "postcreate",
      "permissions": {
        "setting": "private",
        "auth": [
          "create"
        ]
      }
    },
    "/changepost": {
      "name": "/changepost",
      "lambdaFunction": "changepost",
      "permissions": {
        "setting": "open"
      }
    },
    "/delete": {
      "name": "/delete",
      "lambdaFunction": "postdelete",
      "permissions": {
        "setting": "private",
        "auth": [
          "create",
          "read",
          "update",
          "delete"
        ]
      }
    }
  }
}